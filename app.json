{
  "expo": {
    "name": "Gojim",
    "slug": "gojim",
    "version": "2.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/transparent.png",
      "resizeMode": "contain",
      "backgroundColor": "#101313"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "buildNumber": "4",
      "supportsTablet": false,
      "entitlements": {
        "com.apple.developer.networking.wifi-info": true
      },
      "infoPlist": {
        "NSCameraUsageDescription": "Gojim uses your camera for it's barcode scanning capabilities, enable to be able to scan a food barcode and have its nutritional value downloaded",
        "NSMicrophoneUsageDescription": "Allow Gojim to access your microphone"
      },
      "bundleIdentifier": "com.waripamooweidudafa.gojim",
      "appStoreUrl": "https://apps.apple.com/us/app/gojim/id6467132061"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#101313"
      },
      "permissions": ["android.permission.CAMERA"]
    },
    "plugins": [
      [
        "expo-barcode-scanner",
        {
          "cameraPermission": "Gojim uses your camera for it's barcode scanning capabilities, enable to be able to scan a food barcode and have its nutritional value downloaded"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "e5660326-5313-423d-a394-0dd489ad072d"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/e5660326-5313-423d-a394-0dd489ad072d"
    }
  }
}
